{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyS3PublicAcl",
      "Effect": "Deny",
      "Action": ["s3:PutBucketAcl", "s3:PutObjectAcl"],
      "Resource": ["arn:aws:s3:::*", "arn:aws:s3:::*/*"],
      "Condition": {
        "StringEquals": {"s3:x-amz-acl": ["public-read", "public-read-write", "authenticated-read"]}
      }
    }
  ]
}
